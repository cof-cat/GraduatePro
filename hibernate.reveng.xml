<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="T_Address" schema="dbo" catalog="GraduateDB"
		class="edu.td.model.Address">
		<primary-key>
			<generator class="assigned"></generator>
		</primary-key>
	</table>
	<table name="T_User" schema="dbo" catalog="GraduateDB"
		class="edu.td.model.User">
		<primary-key>
			<generator class="assigned"></generator>
		</primary-key>
	</table>
	<table name="T_Uinfo" schema="dbo" catalog="GraduateDB"
		class="edu.td.model.Uinfo">
		<primary-key>
			<generator class="foreign"></generator>
		</primary-key>
	</table>
<table name="View_AllInfo" schema="dbo" catalog="GraduateDB"
	class="edu.td.model.AllInfo">
	<primary-key>
		<generator class="native"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>